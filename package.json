    "test:accessibility": "vitest run --reporter=verbose __tests__/accessibility",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "icons": "node scripts/generate-icons.mjs",
    "soak": "node --experimental-global-webcrypto scripts/soak-runner.mjs",
    "deploy": "./scripts/deploy.sh",
